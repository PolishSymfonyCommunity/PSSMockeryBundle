{
    "name": "polishsymfonycommunity/mockery-bundle",
    "type": "symfony-bundle",
    "description": "This Bundle provides Mockery integration to make it possible to mock service in Behat.",
    "keywords": ["behat", "mockery", "mock", "bdd", "tdd", "test"],
    "homepage": "http://symfonylab.pl",
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub Zalas",
            "email": "jakub@zalas.pl",
            "homepage": "http://www.zalas.eu"
        }
    ],
    "require": {
        "php":                       ">=5.3.2",
        "symfony/framework-bundle":  ">=2.0.0",
        "mockery/mockery":           ">=0.7.0",
        "behat/behat":               "*"
    },
    "autoload": {
        "psr-0": { "PSS\\Bundle\\MockeryBundle": "" }
    },
    "target-dir": "PSS/Bundle/MockeryBundle"
}
